-- Scripts:

local function KWVY_fake_script() -- immisso_2.name and avatar fin 
	local script = Instance.new('LocalScript', immisso_2)

	local immisso = script.parent
	
	local player = game.Players.LocalPlayer
	
	local userid = player.UserId
	local thumType = Enum.ThumbnailType.AvatarBust
	local thumSize = Enum.ThumbnailSize.Size420x420
	local content = game.Players:GetUserThumbnailAsync(userid, thumType, thumSize)
	
	immisso.home.pic.Image = content
	immisso.home.name.Text = player.Name
end
coroutine.wrap(KWVY_fake_script)()
local function BCHBZO_fake_script() -- homebutton.LocalScript 
	local script = Instance.new('LocalScript', homebutton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.supported.TextLabel.Visible = false
		script.Parent.Parent.home.inject.Visible = true
		script.Parent.Parent.home.pic.Visible = true
		script.Parent.Parent.home.name.Visible = true
		end)
end
coroutine.wrap(BCHBZO_fake_script)()
local function USLBMLW_fake_script() -- supportedgames.LocalScript 
	local script = Instance.new('LocalScript', supportedgames)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.supported.TextLabel.Visible = true
		script.Parent.Parent.home.inject.Visible = false
		script.Parent.Parent.home.pic.Visible = false
		script.Parent.Parent.home.name.Visible = false
	end)
end
coroutine.wrap(USLBMLW_fake_script)()
local function PYIEORX_fake_script() -- inject.LocalScript 
	local script = Instance.new('LocalScript', inject)

	script.Parent.MouseButton1Click:Connect(function()
		local loader = {}
		loader.games = {}
		local ts = game:GetService("TweenService")
	
	
		function loader:addGame(id, name, url)
			loader.games[tostring(id)] = {name = name, loadstring = url}
		end
	
		function loader:detectGame()
			local detectedGame = self.games[tostring(game.PlaceId)]
			if detectedGame then
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "immisso",
					Text = "this game is supported! loading the gui now...",
					Duration = 3
				})
				return detectedGame
			else
				return
			end
		end
		loader:addGame(8260276694, "Ability Wars", "aw")
		loader:addGame(10449761463, "The Strongest Battlegrounds", "tsb")
	
		local detectedGame = loader:detectGame()
	
		if detectedGame then
			script.Parent.Parent.Parent:Destroy()
			if not pcall(function()
					loadstring(game:HttpGet("https://raw.githubusercontent.com/ShibaGT/immissorblx/main/".. detectedGame.loadstring))()
				end) then
			end
		else
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "immisso",
				Text = "this game isnt supported.",
				Duration = 3
			})
		end
	end)
end
coroutine.wrap(PYIEORX_fake_script)()
local function TAHC_fake_script() -- immisso_2.Drag 
	local script = Instance.new('LocalScript', immisso_2)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(TAHC_fake_script)()
