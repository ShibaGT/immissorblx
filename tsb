-- Scripts:

local function QKXE_fake_script() -- Main.Drag 
	local script = Instance.new('LocalScript', Main)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(QKXE_fake_script)()
local function RBTTSPE_fake_script() -- KillMost.LocalScript 
	local script = Instance.new('LocalScript', KillMost)

	local Players = game:GetService("Players")
	local player = game.Players.LocalPlayer
	-- Function to teleport player to a target player
	local function teleportToPlayer(targetPlayer)
		local targetCharacter = targetPlayer.Character
		if targetCharacter then
			local targetPart = targetCharacter.PrimaryPart
			if targetPart then
				player.Character:SetPrimaryPartCFrame(targetPart.CFrame)
			end
		end
	end
	
	-- Function to teleport player to all playerss
	local function teleportToAllPlayers()
		wait(0.5)
		for _, targetPlayer in pairs(Players:GetPlayers()) do
			if targetPlayer ~= player then
				teleportToPlayer(targetPlayer)
				wait(0.07)
			end
		end
		wait(0.09)
		player.Character:SetPrimaryPartCFrame(CFrame.new(99999999999999999999999, 999999999999999999999999, 99999999999999999999999))
		wait(0.1)
		player.Character:SetPrimaryPartCFrame(CFrame.new(99999999999999999999999, 999999999999999999999999, 99999999999999999999999))
		wait(0.1)
		player.Character:SetPrimaryPartCFrame(CFrame.new(99999999999999999999999, 999999999999999999999999, 99999999999999999999999))
		wait(3)
		if player.PlayerGui.TSB.Main.Toggles.SafeZoneToggleBG["Tp To Safe Zone After Kill"].BackgroundColor3 == Color3.fromRGB(50, 50, 50) then
			player.Character:SetPrimaryPartCFrame(CFrame.new(-341.7450866699219, 629.573486328125, 70.05008697509766))
		end
		wait(0.5)
		if player.PlayerGui.TSB.Main.Toggles.SafeZoneToggleBG["Tp To Safe Zone After Kill"].BackgroundColor3 == Color3.fromRGB(50, 50, 50) then
			player.Character:SetPrimaryPartCFrame(CFrame.new(-341.7450866699219, 629.573486328125, 70.05008697509766))
		end
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "immisso",
			Text = "should have killed players, if it didn't then try again. ( someone canceled your move )",
			Duration = 5
		})
	end
	
	-- Function to teleport player to a specific CFrame after 1.6 seconds
	local function start()
		if game:GetService("Players").LocalPlayer.Backpack.Scatter then
			local args = {
				[1] = {
					["Goal"] = "Console Move",
					["Tool"] = game:GetService("Players").LocalPlayer.Backpack.Scatter
				}
			}
	
			game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
			teleportToAllPlayers()
		else
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "immisso",
				Text = "make sure to play deadly ninja!",
				Duration = 5
			})
		end
	end
	
	script.Parent.MouseButton1Click:Connect(start)
end
coroutine.wrap(RBTTSPE_fake_script)()
local function UCKNVR_fake_script() -- KillPlayer.LocalScript 
	local script = Instance.new('LocalScript', KillPlayer)

	local Players = game:GetService("Players")
	local player = game.Players.LocalPlayer
	-- Function to teleport player to a target player
	local function teleportToPlayer(targetPlayer)
		local targetCharacter = targetPlayer.Character
		if targetCharacter then
			local targetPart = targetCharacter.PrimaryPart
			if targetPart then
				player.Character:SetPrimaryPartCFrame(targetPart.CFrame)
			end
		end
	end
	
	-- Function to teleport player to all players
	local function killplayer()
		wait(0.5)
		local targetPlayer = Players:FindFirstChild(player.PlayerGui.TSB.Main.TextBoxes.Player.Text)
		if targetPlayer then
			teleportToPlayer(targetPlayer)
		else
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "immisso",
				Text = "player not found.",
				Duration = 5
			})
		end
		wait(0.4)
		player.Character:SetPrimaryPartCFrame(CFrame.new(99999999999999999999999, 999999999999999999999999, 99999999999999999999999))
		wait(1)
		player.Character:SetPrimaryPartCFrame(CFrame.new(99999999999999999999999, 999999999999999999999999, 99999999999999999999999))
		wait(0.1)
		player.Character:SetPrimaryPartCFrame(CFrame.new(99999999999999999999999, 999999999999999999999999, 99999999999999999999999))
		wait(3)
		if player.PlayerGui.TSB.Main.Toggles.SafeZoneToggleBG["Tp To Safe Zone After Kill"].BackgroundColor3 == Color3.fromRGB(50, 50, 50) then
			player.Character:SetPrimaryPartCFrame(CFrame.new(-341.7450866699219, 629.573486328125, 70.05008697509766))
		end
		wait(0.5)
		if player.PlayerGui.TSB.Main.Toggles.SafeZoneToggleBG["Tp To Safe Zone After Kill"].BackgroundColor3 == Color3.fromRGB(50, 50, 50) then
			player.Character:SetPrimaryPartCFrame(CFrame.new(-341.7450866699219, 629.573486328125, 70.05008697509766))
		end
		game:GetService("StarterGui"):SetCore("SendNotification", {
			Title = "immisso",
			Text = "should have killed players, if it didn't then try again. ( someone canceled your move )",
			Duration = 5
		})
	end
	
	-- Function to teleport player to a specific CFrame after 1.6 seconds
	local function start()
		if game:GetService("Players").LocalPlayer.Backpack.Scatter then
			local args = {
				[1] = {
					["Goal"] = "Console Move",
					["Tool"] = game:GetService("Players").LocalPlayer.Backpack.Scatter
				}
			}
	
			game:GetService("Players").LocalPlayer.Character.Communicate:FireServer(unpack(args))
			killplayer()
		else
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "immisso",
				Text = "make sure to play deadly ninja!",
				Duration = 5
			})
		end
	end
	
	script.Parent.MouseButton1Click:Connect(start)
end
coroutine.wrap(UCKNVR_fake_script)()
local function DKAHDDO_fake_script() -- TpToSafeZoneAfterKill.LocalScript 
	local script = Instance.new('LocalScript', TpToSafeZoneAfterKill)

	local player = game.Players.LocalPlayer
	local toggle = player.PlayerGui.TSB.Main.Toggles.SafeZoneBG.SafeZoneToggleBG["Tp To Safe Zone After Kill"]
	
	script.Parent.MouseButton1Click:Connect(function()
		if toggle.BackgroundColor3 == Color3.fromRGB(39, 39, 39) then
			toggle.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
		else
			toggle.BackgroundColor3 = Color3.fromRGB(39, 39, 39)
		end
	end)
end
coroutine.wrap(DKAHDDO_fake_script)()
local function VWJTDY_fake_script() -- NoStun.LocalScript 
	local script = Instance.new('LocalScript', NoStun)

	local player = game.Players.LocalPlayer
	local toggle = player.PlayerGui.TSB.Main.Toggles.NoStunBG.NoStunToggleBG.NoStun
	
	script.Parent.MouseButton1Click:Connect(function()
		if toggle.BackgroundColor3 == Color3.fromRGB(39, 39, 39) then
			toggle.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
		else
			toggle.BackgroundColor3 = Color3.fromRGB(39, 39, 39)
		end
		while(toggle.BackgroundColor3 == Color3.fromRGB(50, 50, 50)) do
			local humanoid = player.Character:FindFirstChild("Humanoid")
			if humanoid then
				if player.Character.Running == true then
					humanoid.WalkSpeed = 25
				else
					humanoid.WalkSpeed = 16
				end
			end
			wait(0)
		end
	end)
end
coroutine.wrap(VWJTDY_fake_script)()
